<header class="navbar">
  <div class="container">
    <nav>
      <ul>
      <%= form_tag(search_page_path, :method => "get") do %>   
      <li><%= link_to "Books", '/books' %></li>
      <li><%= link_to "Contribute", '/books/new' %></li>
      <li><%= search_field_tag :search, params[:search], placeholder: "Search", class: "form-control" %>  
        
       </li>
<% end %>
      <% if logged_in? %>
        <li><%= current_user.name %></li>    
        <li class="login"><%= link_to "LOGOUT", logout_path, method: "delete" %></li>
      <% else %>
        <li class="login"><%= link_to "SIGNUP",   '/users/new' %></li>
        <li class="login"><%= link_to "LOGIN",   '/sessions/new' %></li>
     <% end %>

     <li><a href="#">Update</a></li>
      <li>
          <li><%= link_to current_user.name %></li>
          <ul class="dropdown">
              <li><a href="#">Log out</a></li>
          </ul>
      </li>
      </ul>
    </nav>
  </div>
</header>